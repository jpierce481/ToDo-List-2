if(Ti.version<1.8){alert('Sorry - this application template requires Titanium Mobile SDK 1.8 or later');}
else{var globals={};(function(){var AppTabGroup=require('ui/AppTabGroup').AppTabGroup,ListWindow=require('ui/ListWindow').ListWindow,AddWindow=require('ui/AddWindow').AddWindow;require('db').createDb();globals.tabs=new AppTabGroup({title:'My ToDo List',backgroundColor:'#33FFFF',icon:'images/KS_nav_ui.png',window:new ListWindow({title:'My ToDo List',backgroundColor:'#FFCC66',navBarHidden:false,isDone:0,activity:{onCreateOptionsMenu:function(e){var menu=e.menu;var menuItem=menu.add({title:"Add Task"});menuItem.setIcon("images/ic_menu_add.png");menuItem.addEventListener("click",function(e){new AddWindow().open();});}}})},{title:'My Completed Stuff',icon:'images/KS_nav_views.png',window:new ListWindow({title:'My Completed Stuff',backgroundColor:'#fff',navBarHidden:false,isDone:1})});globals.tabs.open();})();}